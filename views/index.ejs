<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <%- include('./partials/navBar.ejs') %>

    <div>
      <h1>HISTORIQUES</h1>
    </div>
    <div class="contentBlogs">
      <% if(homies.length >0) {%> <% homies.forEach(homy => {%>
      <div class="blogs">
        <a href="/blogs/<%=homy._id%>" class="single">
          <p><%= homy.date.toDateString() %></p>
        </a>
      </div>

      <% }); %> <% }else {%>
      <h2>there are no blogs</h2>
      <%} %>
    </div>
  </body>
</html>
